        android:layout_toStartOf="@+id/editTextTime2"
        android:layout_toLeftOf="@+id/editTextTime2"
        android:fontFamily="serif"
        android:text="In-Time:"
        android:textColor="#000509"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="18dp"
        android:layout_marginRight="18dp"
        android:layout_marginBottom="356dp"
        android:layout_toStartOf="@+id/editTextTime"
        android:layout_toLeftOf="@+id/editTextTime"
        android:fontFamily="serif"
        android:text="Out-Time:"
        android:textColor="#010C15"
        android:textSize="18sp"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/editTextDate2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="419dp"
        android:ems="10"
        android:inputType="date" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="99dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
